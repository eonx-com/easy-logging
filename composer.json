{
    "name": "eonx-com/easy-logging",
    "description": "Centralise logging logic for our lumen projects",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "eonx-com/externals": "^1.0 || ^2.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "^5.5",
        "laravel-doctrine/orm": "^1.4",
        "bugsnag/bugsnag": "^3.19",
        "bugsnag/bugsnag-laravel": "^2.17"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyLogging\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyLogging\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
